  "version": "0.2.0",
  "configurations": [
    {
      "name": "midi-viewer (dev)",
      "type": "node",
      "cwd": "${workspaceFolder}/midi-viewer",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": [
        "dev",
      ],
      "console": "integratedTerminal",
      "skipFiles": [
        "${workspaceFolder}/**/node_modules/**/*.js",
        "<node_internals>/**/*.js"
      ]
    },
    {
      "name": "noteynotes (dev)",
      "type": "node",
      "cwd": "${workspaceFolder}/noteynotes",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": [
        "dev",
      ],
      "console": "integratedTerminal",
      "skipFiles": [
        "${workspaceFolder}/**/node_modules/**/*.js",
        "<node_internals>/**/*.js"
      ]
    }
  ]
}